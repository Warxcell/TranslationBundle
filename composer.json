{
    "name": "arxy/translations-bundle",
    "description": "This bundle provides Tranlations (labels) for project. Plug'n'play",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Bozhidar Hristov",
            "email": "warxcell@gmail.com"
        }
    ],
    "keywords": [
        "translation"
    ],
    "require": {
        "php": "^8.2",
        "doctrine/orm": "~2.5",
        "symfony/framework-bundle": "*",
        "symfony/translation-contracts": "*",
        "symfony/translation": "*"
    },
    "require-dev": {
        "symfony/validator": "*",
        "phpunit/phpunit": "^10.0",
        "doctrine/doctrine-bundle": "*",
        "symfony/yaml": "*",
        "symfony/intl": "*"
    },
    "autoload": {
        "psr-4": {
            "Arxy\\TranslationsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arxy\\TranslationsBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
