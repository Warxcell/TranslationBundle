{
    "name": "arxy/translations-bundle",
    "description": "This bundle provides Tranlations (labels) for project. Plug'n'play",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Bozhidar Hristov",
            "email": "warxcell@gmail.com"
        }
    ],
    "keywords": [
        "translation"
    ],
    "require": {
        "php": "~7.4 | ^8.0",
        "doctrine/orm": "~2.5",
        "symfony/framework-bundle": "*",
        "symfony/translation-contracts": "*",
        "symfony/translation": "*"
    },
    "require-dev": {
        "symfony/validator": "*",
        "phpunit/phpunit": "^9.5",
        "doctrine/doctrine-bundle": "*",
        "symfony/yaml": "^5.3",
        "symfony/intl": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Arxy\\TranslationsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arxy\\TranslationsBundle\\Tests\\": "tests/"
        }
    }
}
